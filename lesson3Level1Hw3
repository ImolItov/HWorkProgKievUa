import java.util.Scanner;
public class lesson3Level1Hw3 {

	public static void main(String[] args) {
		System.out.println("input year");
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		if (a % 4 == 0 && a % 100 > 0) {
			System.out.println("leap");
		} else {
			if (a % 400 == 0) {
				System.out.println("leap");
			} else {
				System.out.println("not leap");
			}
		}
	}
}

public class lesson6Level1Hw1 {

    public static void main(String[] args) {

	int arr[] = { 0, 5, 2, 33, 44, 1, 3, 33 };
	System.out.println(findMax(arr));
    }

    public static int findMax(int arr[]) {
	int max = 0;
	for (int a : arr) {
	    if (a > max) {
		max = a;
	    }
	}
	return max;
    }

}

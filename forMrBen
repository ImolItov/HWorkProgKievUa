import java.util.Arrays;
import java.util.Scanner;

public class treugol {

	public static void main(String[] args) {

		int mas[] = { 0, 5, 2, 4, 7, 1, 3, 19 };
		int sum = 0;
		for (int i : mas) {
			int b[] = { i % 2 }
      for (int result : b) {
				sum = sum + result;
			}
	  System.out.print(sum);
    
		}
	}
}

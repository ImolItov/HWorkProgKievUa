package Lesson2a;

import java.util.Scanner;

public class Lesson2b {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int j = sc.nextInt();
		int n = 7;
    int excess = j % 2;
		for (int i = 0; i < n; i++) {

			if (excess > 0) {
				for (j = 0; j < n; j++) {
					System.out.print("***");
				}

			} else {
				for (j = 0; j < n; j++) {
					System.out.print("+++");
				}
			}
			System.out.println();
		}

	}
}

package com.gmail.imolitov;

import java.util.Scanner;

public class lesson4Level1Hw2 {

	public static void main(String[] args) {

		System.out.println("input quantity  > 4 and < 16");
		Scanner sc = new Scanner(System.in);

		int n = sc.nextInt();
		int s = 1;
		if (4 < n && n < 16) {
			for (int i = n; i >= 1; i--) {
				s = s * i;
			}
			System.out.println("factorial= " + s);
		}
	}
}
